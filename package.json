{
  "name": "circuit-construction-kit-dc",
  "version": "1.3.0-dev.8",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/circuit-construction-kit-dc.git"
  },
  "devDependencies": {
    "grunt": "~1.4.0"
  },
  "phet": {
    "requirejsNamespace": "CIRCUIT_CONSTRUCTION_KIT_DC",
