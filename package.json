    "simulation": true,
    "simFeatures": {
      "supportsSound": true,
      "supportsInteractiveDescription": true
    },
    "supportsOutputJS": true,
    "typescript": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
